{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyCreateRoleOutsidePath",
            "Effect": "Deny",
            "Action": [
                "iam:CreateRole"
            ],
            "Resource": "*",
            "Condition": {
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/developers/*"
                }
            }
        }
    ]
}
